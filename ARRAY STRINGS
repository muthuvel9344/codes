public class sample{
    public static void main(String[]args){
        String[] a []={{"muthu","vel","thiru","sri"},
                    {"murugan","ganth","shek","nadi"},
                    {"naveen","ram","krishna","praveen"},
                    {"ismail","kirupa","santhoes","yogesh"}
                    };
                    
    for(int i=0; i<a.length; i++){
        for(int j=0; j<a.length; j++){
            System.out.println(a[0][1]+"\t");
        }
    }
    }   }
