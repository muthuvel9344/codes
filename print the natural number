#include<stdio.h>
int main(){
    int N;
    int count =1;
    if(scanf("%d",&N)!=1){
        return 1;
    }
    while(count<=N){
        printf("%d",count);
        if(count<N){
            printf("");
            
        }
        count++;
        
    }
      printf("\n");
      return 0;
    }
