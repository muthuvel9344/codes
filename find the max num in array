public class sample{
    public static void main(String[]args){
        int array[]={21,14,16,10};
        int max=array[0];
        for(int i=0;i<array.length;i++){
            if(array[i]>max){
                max=array[i];
            }
        }
        System.out.println("maximum number"+max);
            }
        }
    
