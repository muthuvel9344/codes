 public class sample{
    public static void main(String[]args){
        int [] []a={{2,3,4,1},
                    {4,5,3,7},
                    {7,2,8,5},
                    {8,4,9,7}
                    };
                    
    for(int i=0; i<a.length; i++){
        for(int j=0; j<a.length; j++){
            System.out.print(a[i][j]+"\t");
        }
    }
    }   }
