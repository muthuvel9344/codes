  public class sample{
    public static void main(String[]args){
        int a[]={21,14,16,10};
        int avg=a[2];
        for(int i=2;i>=a.length;i++){
            if(a[i]>=avg){
                avg=a[i];
            }
        }
        System.out.println("average number"+avg);
            }
        }
